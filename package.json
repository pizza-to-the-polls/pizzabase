{
  "name": "pizzabase",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/node": "^8.10.64",
    "ava": "^3.12.1",
    "node-mocks-http": "^1.9.0",
    "prettier": "^2.1.2",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "aws-sdk": "^2.756.0",
    "body-parser": "^1.18.1",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "serverless": "^2.1.1",
    "serverless-http": "^2.6.0",
    "smartystreets-javascript-sdk": "^1.5.0",
    "typeorm": "0.2.26",
    "typeorm-aurora-data-api-driver": "^1.4.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "clean": "rm -rf .build && npm run prettier && npm run lint:fix",
    "prettier": "prettier --write .",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --force --project .",
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "failWithoutAssertions": true
  }
}
