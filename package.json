    "@types/node": "^8.10.64",
    "ava": "^3.12.1",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "serverless-domain-manager": "^5.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^26.4.1",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^3.8"
  },
  "dependencies": {
    "@bugsnag/js": "^7.5.1",
    "@bugsnag/plugin-express": "^7.3.5",
    "aws-sdk": "^2.814.0",
    "body-parser": "^1.18.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "serverless": "^2.1.1",
    "serverless-http": "^2.6.0",
    "smartystreets-javascript-sdk": "^1.5.0",
    "stripe": "^8.119.0",
    "typeorm": "0.2.26",
    "typeorm-aurora-data-api-driver": "^1.4.0",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec npm run start",
    "start": "ts-node src/index.ts",
    "clean": "rm -rf .build && npm run fix",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --force --project .",
    "test": "jest -w 1",
    "test:watch": "jest -w 1 --watchAll"
  }
